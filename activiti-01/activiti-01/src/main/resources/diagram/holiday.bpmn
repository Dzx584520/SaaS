<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE definitions [
        <!ELEMENT definitions (process|bpmndi:BPMNDiagram)*>
        <!ATTLIST definitions
                xmlns CDATA #REQUIRED
                xmlns:xsi CDATA #REQUIRED
                xmlns:xsd CDATA #REQUIRED
                xmlns:activiti CDATA #REQUIRED
                xmlns:bpmndi CDATA #REQUIRED
                xmlns:omgdc CDATA #REQUIRED
                xmlns:omgdi CDATA #REQUIRED
                typeLanguage CDATA #REQUIRED
                expressionLanguage CDATA #REQUIRED
                targetNamespace CDATA #REQUIRED>
        <!ELEMENT process (startEvent|userTask|sequenceFlow|endEvent)*>
        <!ATTLIST process
                id CDATA #REQUIRED
                name CDATA #REQUIRED
                isExecutable CDATA #REQUIRED>
        <!ELEMENT startEvent (#PCDATA)>
        <!ATTLIST startEvent
                id CDATA #REQUIRED
                name CDATA #REQUIRED>
        <!ELEMENT userTask (#PCDATA)>
        <!ATTLIST userTask
                activiti:assignee CDATA #REQUIRED
                id CDATA #REQUIRED
                name CDATA #REQUIRED>
        <!ELEMENT sequenceFlow (#PCDATA)>
        <!ATTLIST sequenceFlow
                id CDATA #REQUIRED
                sourceRef CDATA #REQUIRED
                targetRef CDATA #REQUIRED>
        <!ELEMENT endEvent (#PCDATA)>
        <!ATTLIST endEvent
                id CDATA #REQUIRED
                name CDATA #REQUIRED>
        <!ELEMENT bpmndi:BPMNDiagram (bpmndi:BPMNPlane)*>
        <!ATTLIST bpmndi:BPMNDiagram
                id CDATA #REQUIRED>
        <!ELEMENT bpmndi:BPMNPlane (bpmndi:BPMNShape|bpmndi:BPMNEdge)*>
        <!ATTLIST bpmndi:BPMNPlane
                bpmnElement CDATA #REQUIRED
                id CDATA #REQUIRED>
        <!ELEMENT bpmndi:BPMNShape (omgdc:Bounds)*>
        <!ATTLIST bpmndi:BPMNShape
                bpmnElement CDATA #REQUIRED
                id CDATA #REQUIRED>
        <!ELEMENT omgdc:Bounds (#PCDATA)>
        <!ATTLIST omgdc:Bounds
                height CDATA #REQUIRED
                width CDATA #REQUIRED
                x CDATA #REQUIRED
                y CDATA #REQUIRED>
        <!ELEMENT bpmndi:BPMNEdge (omgdi:waypoint)*>
        <!ATTLIST bpmndi:BPMNEdge
                bpmnElement CDATA #REQUIRED
                id CDATA #REQUIRED>
        <!ELEMENT omgdi:waypoint (#PCDATA)>
        <!ATTLIST omgdi:waypoint
                x CDATA #REQUIRED
                y CDATA #REQUIRED>
        ]>
